	background: -webkit-gradient(linear, left top, left bottom, color-stop(0%,#ffe66a), color-stop(100%,#ffbf2d)); /* Chrome,Safari4+ */
	background: -webkit-linear-gradient(top,  #ffe66a 0%,#ffbf2d 100%); /* Chrome10+,Safari5.1+ */
	background: -o-linear-gradient(top,  #ffe66a 0%,#ffbf2d 100%); /* Opera 11.10+ */
	background: -ms-linear-gradient(top,  #ffe66a 0%,#ffbf2d 100%); /* IE10+ */
	background: linear-gradient(to bottom,  #ffe66a 0%,#ffbf2d 100%); /* W3C */
	filter: progid:DXImageTransform.Microsoft.gradient( startColorstr='#ffe66a', endColorstr='#ffbf2d',GradientType=0 ); /* IE6-9 */

}

