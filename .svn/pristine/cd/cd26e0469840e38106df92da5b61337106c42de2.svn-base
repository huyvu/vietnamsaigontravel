.tour-thumbnail .btn-summary {
	background: #ffde56; /* Old browsers */
	background: -moz-linear-gradient(top,  #ffde56 0%, #ffaa22 100%); /* FF3.6+ */
	background: -webkit-gradient(linear, left top, left bottom, color-stop(0%,#ffde56), color-stop(100%,#ffaa22)); /* Chrome,Safari4+ */
	background: -webkit-linear-gradient(top,  #ffde56 0%,#ffaa22 100%); /* Chrome10+,Safari5.1+ */
	background: -o-linear-gradient(top,  #ffde56 0%,#ffaa22 100%); /* Opera 11.10+ */
	background: -ms-linear-gradient(top,  #ffde56 0%,#ffaa22 100%); /* IE10+ */
	background: linear-gradient(to bottom,  #ffde56 0%,#ffaa22 100%); /* W3C */
	filter: progid:DXImageTransform.Microsoft.gradient( startColorstr='#ffde56', endColorstr='#ffaa22',GradientType=0 ); /* IE6-9 */
