nav {
	border-top: 1px solid #f0eeee;
	box-shadow: 0px 2px 3px #757575;
	z-index: 999;
	position: relative;
}

nav .menu {
	display: block;

}

nav .menu li {
	list-style: none;
}

nav .menu > li {
	display: table-cell;
}

nav .menu > li > a {
	color: #666;
	display: block;
	font-size: 12px;
	font-weight: bold;
	line-height: 50px;
	padding: 0 11px;
	text-transform: uppercase;
	text-decoration: none;
}

nav .menu > li > ul {
	/*visibility: hidden;*/
	overflow: hidden;
	width: 200px;
	position: absolute;
	max-height:0;
	z-index: 99999;
	-webkit-transition: max-height 0.4s ease-in-out 0s;
	   -moz-transition: max-height 0.4s ease-in-out 0s;
	    -ms-transition: max-height 0.4s ease-in-out 0s;
	     -o-transition: max-height 0.4s ease-in-out 0s;
	        transition: max-height 0.4s ease-in-out 0s;
}

nav .menu > li > a:hover {
	background: #eee;
	color: #0daa60;
}

nav .menu > li:hover > ul {
	max-height: 400px;
}

nav .menu li ul li a {
	background: #444;
	border-top: 1px solid #666;
	color: #e5e5e5;
	display: block;
	font-size: 12px;
	font-weight: bold;
	line-height: 32px;
	padding: 0 10px;
	text-transform: uppercase;
	text-decoration: none;
}

nav .menu li ul li:hover a {
	background: #333;
}

/*Footer*/

footer .footer-img {
